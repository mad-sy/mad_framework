{
    "name": "mad-sy/mad_framework",
    "description": "A mini PHP Framework made by MAD for students",
    "keywords": [
        "MAD",
        "framework"
    ],
    "type": "project",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "require": {
        "spatie/ignition": "^1.15",
        "league/container": "^4.2",
        "adbario/php-dot-notation": "^3.3",
        "vlucas/phpdotenv": "^5.6",
        "laminas/laminas-diactoros": "^3.5",
        "league/route": "^5.1",
        "laminas/laminas-httphandlerrunner": "^2.11",
        "twig/twig": "^3.0",
        "illuminate/database": "^10.0",
        "illuminate/events": "^10.0",
        "symfony/http-foundation": "^7.1",
        "cartalyst/sentinel": "^7.0",
        "slim/csrf": "^1.5",
        "respect/validation": "^2.3",
        "illuminate/pagination": "^10.48"
    }, "extra": {
        "mad_framework": {
            "dont-discover": []
        }
    }
}