{% extends 'layouts/app.twig' %}

{% block title %}
    Home
{% endblock %}

{% block content %}
    {% if users is not empty %}
        {% for user in users %}
            <div>
                <a href="#">
                    {{ user.first_name }}
                </a>
            </div>
        {% endfor %}

        {{ users.links() | raw }}
    {% endif %}
{% endblock %}
